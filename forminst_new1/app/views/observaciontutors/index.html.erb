<p id="notice"><%= notice %></p>

<h1>Listing Observaciontutors</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Informeactividadid</th>
      <th>Observacion</th>
      <th>Fecha</th>
      <th>Actual</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @observaciontutors.each do |observaciontutor| %>
      <tr>
        <td><%= observaciontutor.id %></td>
        <td><%= observaciontutor.informeActividadId %></td>
        <td><%= observaciontutor.observacion %></td>
        <td><%= observaciontutor.fecha %></td>
        <td><%= observaciontutor.actual %></td>
        <td><%= link_to 'Show', observaciontutor %></td>
        <td><%= link_to 'Edit', edit_observaciontutor_path(observaciontutor) %></td>
        <td><%= link_to 'Destroy', observaciontutor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Observaciontutor', new_observaciontutor_path %>
