<script type="text/javascript">
    function mostrarFecha(op) {
        var date = new Date(op.substring(0, 4),op.substring(5, 7) - 1,op.substring(8, 10));
        date.setDate(date.getDate() + 180);
        var dd = date.getDate();
        var mm = date.getMonth() + 1;
        var y = date.getFullYear();
        var hola = y.toString() + '-' + mm.toString() + '-' + dd.toString();
        document.getElementById("nuevaFecha").value=hola;
    }
</script>

