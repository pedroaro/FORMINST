<div class = 'row'>
  <div class="col-xs-11 col-xs-offset-1 col-md-12 col-md-offset-0 text-justify">
    <h1 class="text-left">Detalles del Informe</h1>
  </div>
</div>
<div class = 'row'>
  <div class="col-xs-11 col-xs-offset-1 col-md-12 col-md-offset-0 text-center">
    <h5><strong><%=session[:nombre_informe]%> DE ACTIVIDADES REALIZADAS POR EL INSTRUCTOR DURANTE LA EJECUCIÓN DE SU PLAN DE FORMACIÓN Y CAPACITACIÓN</strong></h5>
  </div>
</div>
<div class = 'row'>
  <div class="col-xs-11 col-xs-offset-1 col-md-12 col-md-offset-0 text-left">
    <h5><strong>Estado del Documento: <%=@status.concepto%></strong></h5>
  </div>
</div>
<br/>
<div class= "row">
  <div class="col-xs-12 col-md-12 col-lg-12">
      <%= form_tag({controller: 'informes', action: 'actualizar_informe'}) do%>
          <input id="cant_docencia" type="hidden" name="cant_docencia" value="<%=@actividadesadoc.size()%>"/> 
          <input id="cant_investigacion" type="hidden" name="cant_investigacion" value="<%=@actividadesainv.size()%>"/>
          <input id="cant_formacion" type="hidden" name="cant_formacion" value="<%=@actividadesafor.size()%>"/> 
          <input id="cant_extension" type="hidden" name="cant_extension" value="<%=@actividadesaext.size()%>"/> 
          <input id="cant_otra" type="hidden" name="cant_otra" value="<%=@actividadesaotr.size()%>"/>
          <input id="cant_obligatoria" type="hidden" name="cant_obligatoria" value="<%=@actividadesaobli.size()%>"/>
          <input id="cant_result" type="hidden" name="cant_result" value="<%=@resultados.size()%>"/> 
          <input id="tipoinf" type="hidden" name="tipoinf" value="<%=@tipo_informe%>"/> 
          <input id="numeroinf" type="hidden" name="numero_informe" value="<%=@numero_informe%>"/> 
          <table id="cprtabla" class="table table-bordered ">
            <tbody>
              <tr>
                <td style='width:50%;' class="text-center"><strong>Fecha de inicio</strong></td>
                <td style='width:50%;' class="text-center"><strong>Fecha fin</strong></td>
              </tr>

              <tr>
                <td class="text-center">
                  <% if @numero_informe == 3 || @numero_informe == 6 || @numero_informe == 7 %>
                    <textarea class="form-control" rows="1" disabled><%= @fecha1 %></textarea>
                  <% else %>
                    <%if @bool_enviado == 0 %>
                     <div id="sandbox-container">
                        <div class="input-group date">
                            <input type="text" class="form-control" name="fechaIni" value="<%= @fecha1 %>" onchange="mostrarFecha(this.value)"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                        </div>
                      </div>
                    <%else%>
                      <div id="sandbox-container">
                        <div class="input-group date">
                            <input type="text" class="form-control" name="fechaIni" value="<%= @fecha1 %>" onchange="mostrarFecha(this.value)" disabled="true"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                        </div>
                      </div>
                    <%end%>
                  <% end %>
                </td>
                <td class="text-center">
                  <% if @numero_informe == 3 || @numero_informe == 6 || @numero_informe == 7 %>
                    <textarea class="form-control" rows="1" disabled><%= @fecha2 %></textarea>
                  <% else %>
                    <%if @bool_enviado == 0 %>
                     <div id="sandbox-container">
                        <div class="input-group date">
                            <input type="text" class="form-control" name="fechaFin" value="<%= @fecha2 %>" onchange="mostrarFecha(this.value)"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                        </div>
                      </div>
                    <%else%>
                      <div id="sandbox-container">
                        <div class="input-group date">
                            <input type="text" class="form-control" name="fechaFin" value="<%= @fecha2 %>" onchange="mostrarFecha(this.value)" disabled="true"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                        </div>
                      </div>
                    <%end%>
                  <% end %>
                </td>
              </tr>
            </tbody>
          </table>
          <table id="cprtabla" class="table table-bordered ">
            <tbody>
              <tr></tr>
              <tr>
                <td class="text-center" colspan="5"><h5><strong> Actividades a realizar por el instructor:</strong></h5></td>
              </tr>
              <tr class='title-table'>

                <td class="text-center"  style='width:30%;'><strong>Actividad Docente Programada</strong></td>
                <td class="text-center"  style='width:30%;'><strong>Actividad Docente Ejecutada</strong></td>
                <td class="text-center"  style='width:30%;'><strong>Observaciones</strong></td>
                <td class="text-center"  style='width:30%;'><strong>Más observaciones</strong></td>
                <td class="text-center"  style='width:30%;'><strong>Soportes</strong></td>
              </tr>
                <% @actividadesadoc.each_with_index do |actdoc, i| %>
                  <% if @semestres[actdoc.id] == 5 && @CJagregado[0]== "no" %>
                    <tr class='title-table'>
                      <td class="text-center" colspan="5"> <strong>Obligatorias</strong> </td>
                    <tr>
                    <% @CJagregado[0]= "si" %>
                  <% end %>
                  <tr>
                    <td>
                      <textarea class="form-control" rows="3" name='docencia<%= actdoc.id.to_s %>' id ='<%= actdoc.id %>' disabled><%= actdoc.actividad %></textarea>
                    </td>
                    <td>
                      <% if @bool_enviado == 0 %>
                        <textarea class="form-control" rows="3" name='ejecutada<%= actdoc.id.to_s%>'><%= @actividadese[@j].descripcion %></textarea>
                      <% else %>
                        <textarea class="form-control" rows="3" name='ejecutada<%= actdoc.id.to_s%>' disabled><%= @actividadese[@j].descripcion %></textarea>
                      <% end %>
                    </td>
                    <td>
                      <% if @bool_enviado == 0 %>
                        <textarea class="form-control" rows="3" name='observacion<%= actdoc.id.to_s%>' ><%=@observaciont[@j] %></textarea>
                      <% else %>
                        <textarea class="form-control" rows="3" name='observacion<%= actdoc.id.to_s%>' disabled><%=@observaciont[@j] %></textarea>
                      <% end %>
                    </td>
                    <td>
                      <% if @observacionesExtras[@j] != "si" %>
                        <%= link_to "Más obs. ", {:controller => "informes", :action => "mas_observaciones", :actividad_id => actdoc.id, :informe_id => @informe.id }, class: "pad add btn btn-default" %>
                      <% else %>
                        <%= link_to "Más obs. ", {:controller => "informes", :action => "mas_observaciones", :actividad_id => actdoc.id, :informe_id => @informe.id }, class: "pad add btn btn-warning" %>
                      <% end %>
                    </td>
                    <td>
                      <%= link_to "Soportes", {:controller => "documents", :action => "index", :actividad_id => actdoc.id }, class: "pad add btn btn-default" %>
                    </td>
                    <input type="hidden" name="doc<%=i.to_s%>" value='<%= actdoc.id %>'>
                    
                  </tr>
                  <% @j= @j +1 %>
                <% end %>

              <tr><td colspan="5"></td></tr>
    
              <tr class='title-table'>
                <td class="text-center"><strong>Actividad de Investigación Programada</strong></td>
                <td class="text-center"><strong>Actividad de Investigación Ejecutada</strong></td>
                <td class="text-center"><strong>Observaciones</strong></td>
                <td class="text-center"  style='width:30%;'><strong>Más observaciones</strong></td>
                <td class="text-center"  style='width:30%;'><strong>Soportes</strong></td>
              </tr>

              <% @actividadesainv.each_with_index do |actdoc,i| %>
                <tr>
                  <td>
                      <textarea class="form-control" rows="3" name='investigacion<%= actdoc.id.to_s %>' id ='<%= actdoc.id %>' disabled><%= actdoc.actividad %></textarea>
                    </td>
                    <td>
                      <textarea class="form-control" rows="3" name='ejecutada<%= actdoc.id.to_s%>' ><%= @actividadese[@j].descripcion %></textarea>
                    </td>
                    <td>
                      <% if @bool_enviado == 0 %>
                        <textarea class="form-control" rows="3" name='observacion<%= actdoc.id.to_s%>' ><%=@observaciont[@j] %></textarea>
                      <% else %>
                        <textarea class="form-control" rows="3" name='observacion<%= actdoc.id.to_s%>' disabled><%=@observaciont[@j] %></textarea>
                      <% end %>
                    </td>
                    <td>
                      <% if @observacionesExtras[@j] != "si" %>
                        <%= link_to "Más obs. ", {:controller => "informes", :action => "mas_observaciones", :actividad_id => actdoc.id, :informe_id => @informe.id }, class: "pad add btn btn-default" %>
                      <% else %>
                        <%= link_to "Más obs. ", {:controller => "informes", :action => "mas_observaciones", :actividad_id => actdoc.id, :informe_id => @informe.id }, class: "pad add btn btn-warning" %>
                      <% end %>
                    </td>
                    <td>
                      <%= link_to "Soportes", {:controller => "documents", :action => "index", :actividad_id => actdoc.id }, class: "pad add btn btn-default" %>
                    </td>
                    <input type="hidden" name="inv<%=i.to_s%>" value='<%= actdoc.id %>'>
                </tr>

                  <input type="hidden" name="result<%=i.to_s%>" value='-1'>

                  <tr>
                  <td colspan="5">
                  <% cpcont = 1 %>
                  <table id="cpResultados<%=i.to_s%>" class="table table-bordered" style='width:300%;'> 
                  <tbody>
                    <% if !@resultados[@j].blank? %>
                      <% @resultados[@j].each do |cjResultado|  %>
                        <tr id="cpfila<%=cpcont.to_s%><%=i.to_s%>">
                        <td>
                          <div id="cpseleccion<%=cpcont.to_s%><%=i.to_s%>" class="form-group"> 
                          <label>Seleccione El tipo de resultado:</label> 
                          <select name="tipo<%=cpcont.to_s%><%=i.to_s%>" option="<%=cjResultado.tipo_resultado_id.to_s%>" onchange="mostrarInputs(this.value, <%=i.to_s%> , <%=cpcont.to_s%>)" class="form-control" id="sel<%=cpcont.to_s%><%=i.to_s%>" style="width:65%;"> 
                            <option>Seleccione un tipo</option> 
                            <option value="1" <% if cjResultado.tipo_resultado_id.to_s == "1" %> selected="selected" <%end%> > Trabajos publicados (Libros, revistas, artículos de prensa, CDs, manuales, publicaciones electrónicas, etc.)</option> 
                            <option value="2" <% if cjResultado.tipo_resultado_id.to_s == "2" %> selected="selected" <%end%> >Presentación de ponencias, conferencias y carteles en eventos científicos</option> 
                            <option value="3" <% if cjResultado.tipo_resultado_id.to_s == "3" %> selected="selected" <%end%> > Presentación de informes técnicos</option> 
                            <option value="4" <% if cjResultado.tipo_resultado_id.to_s == "4" %> selected="selected" <%end%> >Otros</option> 
                            <option value="5" <% if cjResultado.tipo_resultado_id.to_s == "5" %> selected="selected" <%end%> >Asistencia a eventos científicos (Congresos, Seminarios, Coloquios, Jornadas, etc.)</option> 
                            <option value="6" <% if cjResultado.tipo_resultado_id.to_s == "6" %> selected="selected" <%end%> >Organización de eventos científicos (Congresos, Seminarios, Coloquios, Jornadas, etc.)</option> 
                            <option value="7" <% if cjResultado.tipo_resultado_id.to_s == "7" %> selected="selected" <%end%> > Dictado de cursos, talleres o seminarios científicos</option> 
                          </select> 
                        </div> 

                        <div id="tipos<%=cpcont.to_s%><%=i.to_s%>" class="form-group" style="display: none;"> 
                          <label>Tipo de Trabajo Publicado que desea registrar:</label>
                          <br> 
                          <select id="tipo_publicacion<%=cpcont.to_s%><%=i.to_s%>" name="resultado_tipos<%=cpcont.to_s%><%=i.to_s%>" onchange="mostrarInputsTipos(this.value, <%=i.to_s%> , <%=cpcont.to_s%>)" class="form-control" style="width:20%;"> 
                            <option>Seleccione un tipo</option> 
                            <option value="1" <% if cjResultado.tipo_publicacion.to_s == "1" %> selected="selected" <%end%> >Libro</option> 
                            <option value="2" <% if cjResultado.tipo_publicacion.to_s == "2" %> selected="selected" <%end%> >Articulo de Revista o Journals</option> 
                            <option value="3" <% if cjResultado.tipo_publicacion.to_s == "3" %> selected="selected" <%end%> >Articulos de Prensa</option> 
                            <option value="4" <% if cjResultado.tipo_publicacion.to_s == "4" %> selected="selected" <%end%> >CD</option> 
                            <option value="5" <% if cjResultado.tipo_publicacion.to_s == "5" %> selected="selected" <%end%> >Manuales</option> 
                            <option value="6" <% if cjResultado.tipo_publicacion.to_s == "6" %> selected="selected" <%end%> >Publicaciones Electronicas</option> 
