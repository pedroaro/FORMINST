                        <textarea class="form-control" rows="3" name='ejecutada<%= actdoc.id.to_s%>' ><%= @actividadesainvejec[i].descripcion%></textarea>
                      </td>
                    <% else %>
                      <td>
                        <textarea class="form-control" rows="3" name='ejecutada<%= actdoc.id.to_s%>' ></textarea>
                      </td>
                    <% end %>
                    <td>
                      <textarea class="form-control" rows="3" name='observacion<%= actdoc.id.to_s%>' ></textarea>
                    </td>
                    <input type="hidden" name="inv<%=i.to_s%>" value='<%= actdoc.id %>'>
                </tr>

                  <input type="hidden" name="result<%=i.to_s%>" value='-1'>

                  <tr>
                  <td colspan="4">
                  <% cpcont = 1 %>
                  <table id="cpResultados<%=i.to_s%>" class="table table-bordered" style='width:300%;'> 
                  <tbody>
                    <tr>
                      <td>
                        <a id="cpagregar<%=i.to_s%>" class="btn btn-default resultado" rel="<%=i.to_s%>" rel2="<%=(cpcont - 1).to_s%>"><span class="glyphicon glyphicon-plus"></span> Agregar Resultado</a>
