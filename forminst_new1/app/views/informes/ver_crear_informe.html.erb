              <tr>
                <td colspan="4">
                    <textarea name="conclusiones" class="form-control" rows="3" ></textarea>
                </td>
              </tr>
              <tr>
                <td colspan="4"><b>Opinión tutor:</b></td>
              </tr>
              <tr>
                <td colspan="4">
                    <textarea name="opinion" class="form-control" rows="3" ></textarea>
                </td>
              </tr>
              <tr>
                <td colspan="4"><b>Justificaciones (Extensión del Período del Informe)</b></td>
              </tr>
              <tr>
                <td colspan="4">
                    <textarea name="justi" class="form-control" rows="3" ></textarea>
                </td>
              </tr>
          

            </tbody>
          </table>
      <div class = 'row'>
          <center><button type="submit" class="btn btn-forminst btn-lg">Guardar</button></center>
      </div>
    <% end %>
  </div>
</div>

