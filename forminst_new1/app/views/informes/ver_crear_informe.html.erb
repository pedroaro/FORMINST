                      <textarea class="form-control" rows="3" name='ejecutada<%= actdoc.id.to_s%>' ><%= @actividadesaotrejec[i].descripcion%></textarea>
                    </td>
                  <% else %>
                    <td colspan="2">
                      <textarea class="form-control" rows="3" name='ejecutada<%= actdoc.id.to_s%>' ></textarea>
                    </td>
                  <% end %>
                  <input type="hidden" name="otra<%=i.to_s%>" value='<%= actdoc.id %>'>
                </tr>
              <% end %>

              <tr>
                <td colspan="4"><b>Conclusiones:</b></td>
