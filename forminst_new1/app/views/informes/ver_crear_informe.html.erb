

<div class="row">
  <div class="col-xs-12 col-md-12 col-md-offset-0">
    <div class= "text-left">
      <%if @informes%>
        <div>
          <span><%= link_to 'Plan Formación', :controller => 'iniciotutor', :action => 'planformacions' %></span>
          <p style="display:inline"> / </p>
          <span><%= link_to session[:instructorName] , :controller => 'iniciotutor', :action => 'ver_detalles_plan' %></span>
          <p style="display:inline"> / </p>
          <span><%= link_to 'Informes', :controller => 'informes', :action => 'listar_informes' %></span>
          <p style="display:inline">  /  <%= @nombre_informe  %></p>
        </div>
      <%end%>
    </div>
    <div class="text-right">
      <div id = "fechamuestra">
        <h3> <%= Time.now.strftime("%d") %>
          <% if Time.now.strftime("%m").to_i == 1 %>
            de Enero del
          <% elsif Time.now.strftime("%m").to_i == 2 %>
            de Febrero del
          <% elsif Time.now.strftime("%m").to_i == 3 %>
            de Marzo del
          <% elsif Time.now.strftime("%m").to_i == 4 %>
            de Abril del
          <% elsif Time.now.strftime("%m").to_i == 5 %>
            de Mayo del
          <% elsif Time.now.strftime("%m").to_i == 6 %>
            de Junio del
          <% elsif Time.now.strftime("%m").to_i == 7 %>
            de Julio del
          <% elsif Time.now.strftime("%m").to_i == 8 %>
            de Agosto del
          <% elsif Time.now.strftime("%m").to_i == 9 %>
            de Septiembre del
          <% elsif Time.now.strftime("%m").to_i == 10 %>
            de Octubre del
          <% elsif Time.now.strftime("%m").to_i == 11 %>
            de Noviembre del
          <% elsif Time.now.strftime("%m").to_i == 12 %>
            de Diciembre del
          <% end %>
          <%= Time.now.strftime("%Y") %> 
        </h3>
      </div>
      <%if @informes%>
        <div>
          <h4><b><%=session[:nombre_usuario] %></b></h4>
        </div>
      <%end%>
    </div>
  </div>
</div>

<div class = 'row'>
  <div class="col-xs-11 col-xs-offset-1 col-md-12 col-md-offset-0 text-justify">
    <h1 class="text-left">Crear Informe <%=@nombre_informe%></h1>
    <h1 class="text-left"> </h1>
  </div>
</div>
<br>

<div class= "row">
  <div class="col-xs-12 col-md-12 col-lg-12">
    <%= form_tag({controller: 'informes', action: 'crear_informe'}) do%>
        <input id="cant_docencia" type="hidden" name="cant_docencia" value="<%=@actividadesadoc.size()%>"/> 
        <input id="cant_investigacion" type="hidden" name="cant_investigacion" value="<%=@actividadesainv.size()%>"/>
        <input id="cant_formacion" type="hidden" name="cant_formacion" value="<%=@actividadesafor.size()%>"/> 
        <input id="cant_extension" type="hidden" name="cant_extension" value="<%=@actividadesaext.size()%>"/> 
        <input id="cant_obligatoria" type="hidden" name="cant_obligatoria" value="<%=@actividadesaobli.size()%>"/> 
        <input id="cant_otra" type="hidden" name="cant_otra" value="<%=@actividadesaotr.size()%>"/> 
        <input id="tipoinf" type="hidden" name="tipoinf" value="<%=@tipo_informe%>"/> 
        <input id="tipoinf" type="hidden" name="numero_informe" value="<%=@numero_informe%>"/> 

        <table id="cprtabla" class="table table-bordered ">
          <tbody>
            <tr>
              <td style='width:50%;' class="text-center"><strong>Fecha de inicio</strong></td>
              <td style='width:50%;' class="text-center"><strong>Fecha fin</strong></td>
            </tr>

            <tr>
              <td class="text-center">
                <% if @numero_informe == 3 || @numero_informe == 6 || @numero_informe == 7 %>
                  <textarea class="form-control" rows="1" disabled><%= @fecha_inicio1 %></textarea>
                <% else %>
                  <div id="sandbox-container">
                    <div class="input-group date">
                        <input type="text" class="form-control" name="fechaIni" onchange="mostrarFecha(this.value)"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                    </div>
                  </div>
                <% end %>
              </td>
              <td class="text-center">
              	<% if @numero_informe == 3 || @numero_informe == 6 || @numero_informe == 7 %>
                    <textarea class="form-control" rows="1" disabled><%= @fecha_fin1 %></textarea>
                <% else %>
                    <div id="sandbox-container">
                    <div class="input-group date">
                      <input type="text" class="form-control" name="fechaFin" onchange="mostrarFecha(this.value)"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
                    </div>
                  </div>
                <% end %>
              </td>
            </tr>
          </tbody>
        </table>
        <table id="cprtabla" class="table table-bordered ">
            <tbody>
              <tr></tr>

              <tr>
                <td class="text-center" colspan="4"><h5><strong> Actividades a realizar por el instructor:</strong></h5></td>
              </tr>
              <tr class='title-table'>

                <td class="text-center"  style='width:30%;'><strong>Actividad Docente Programada</strong></td>
                <td class="text-center"  style='width:30%;'><strong>Actividad Docente Ejecutada</strong></td>
                <td class="text-center"  style='width:30%;'><strong>Observaciones</strong></td>
              </tr>
                <% @actividadesadoc.each_with_index do |actdoc, i| %>
                  <tr>
                    <td>
                      <textarea class="form-control" rows="3" name='docencia<%= actdoc.id.to_s %>' id ='<%= actdoc.id %>' disabled><%= actdoc.actividad %></textarea>
                    </td>
                    <% if  (@numero_informe == 3 || @numero_informe == 6 || @numero_informe == 7 ) &&  @actividadesadocejec[i] != []%>
                      <td>
                        <textarea class="form-control" rows="3" name='ejecutada<%= actdoc.id.to_s%>' ><%= @actividadesadocejec[i].descripcion%></textarea>
                      </td>
                    <% else %>
                      <td>
                        <textarea class="form-control" rows="3" name='ejecutada<%= actdoc.id.to_s%>' ></textarea>
                      </td>
                    <% end %>
                    <td>
                      <textarea class="form-control" rows="3" name='observacion<%= actdoc.id.to_s%>' ></textarea>
                    </td>
                    <input type="hidden" name="doc<%=i.to_s%>" value='<%= actdoc.id %>'>
                    
                  </tr>
                <% end %>

              <tr><td colspan="3"></td></tr>
    
